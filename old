 1 min   59999999.99999879
 1 hour  3599999999.9999714
 1 day   86400000000.00056
 1 month 2629745999999.9805
 1 year  31556952000000.43
 2^64 - 1 = 18,446,744,073,709,551,615 

 auto time = (year-1970) * 31556952000000.43 + \
 (mon)      *  2629745999999.9805 + \
 (mday-1)           *    86400000000.00056 + \
 (hour)           *     3599999999.9999714 + \
 min            *       59999999.99999879 + \
 sec            *        1000000 + \
 us;

 return (year-1970) * 31556952000000 + \
 (mon-1)      *  2592000000000 + \
 (mday)       *    86400000000 + \
 (hour+21)    *     3600000000 + \
 (min)        *       60000000 + \
 (sec)        *        1000000 + \
 us;
